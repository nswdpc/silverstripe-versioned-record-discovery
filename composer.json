{
    "name": "nswdpc/silverstripe-versioned-record-discovery",
    "description": "Versioned record discovery in a Silverstripe install. Includes unpublished record reports and review/revert versioned dataobjects to a selected version",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "unpublished",
        "versioned",
        "modeladmin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/admin" : "^1",
        "silverstripe/versioned" : "^1",
        "silverstripe/reports" : "^4",
        "lekoala/silverstripe-cms-actions": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^7",
        "friendsofphp/php-cs-fixer" : "^2.1",
        "squizlabs/php_codesniffer": "^3.0"
    }
}
